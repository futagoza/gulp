{
    "name": "@futagoza/gulp",
    "private": true,
    "workspaces": [
        "gulpx/*",
        "packages/*",
        "plugins/*"
    ],
    "scripts": {
        "lint": "gulp lint",
        "bump-major": "gulp bump --major",
        "bump-minor": "gulp bump --minor",
        "bump-patch": "gulp bump --patch",
        "rollout": "gulp publish",
        "rollout-dry": "gulp publish --dry"
    },
    "devDependencies": {
        "eslint-config-futagozaryuu": "5",
        "gulp-eslint": "5.0.0",
        "pump": "3.0.0"
    },
    "engines": {
        "node": ">= 6"
    }
}
